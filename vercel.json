{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "server/src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1",
      "check": true
    },
    {
      "src": "/(.*)",
      "dest": "/client/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "FACEBOOK_APP_ID": "2955016021354200",
    "FACEBOOK_PAGE_ID": "928400333684435",
    "INSTAGRAM_BUSINESS_ACCOUNT_ID": "17841478489499957",
    "CLOUDINARY_CLOUD_NAME": "ddvtwoyxp",
    "CLOUDINARY_API_KEY": "388346273598741"
  }
}
